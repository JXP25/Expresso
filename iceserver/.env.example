# Coturn ICE Server Configuration
# Replace these values with your actual configuration

# External IP address of your server (public IP)
EXTERNAL_IP=YOUR_SERVER_PUBLIC_IP

# TURN server credentials
TURN_USERNAME=your_turn_username
TURN_PASSWORD=your_secure_password

# TURN realm (usually your domain)
TURN_REALM=yourdomain.com



# TLS Certificate paths (required for HTTPS frontend)
TLS_CERT_PATH=/etc/coturn/cert.pem
TLS_KEY_PATH=/etc/coturn/privkey.pem 